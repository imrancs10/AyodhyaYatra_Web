@model IEnumerable<DataLayer.Visitor_Detail>

@{
    ViewBag.Title = "VisitorPass";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>VisitorPass</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Client_IP_Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Session_Start_Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Browser_Type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Device_Type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Operating_System)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Client_IP_Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Session_Start_Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Browser_Type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Device_Type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Operating_System)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
