        $(document).ready(function () {
            // Store original font sizes
            $('*').each(function () {
                $(this).data('original-font-size', $(this).css('font-size'));
            });

            $('#font-lg').click(function () {
                $('*').each(function () {
                    var currentSize = parseFloat($(this).css('font-size'));
                    $(this).css('font-size', (currentSize * 1.1) + 'px');
                });
            });

            $('#font-sm').click(function () {
                $('*').each(function () {
                    var currentSize = parseFloat($(this).css('font-size'));
                    $(this).css('font-size', (currentSize * 0.9) + 'px');
                });
            });

            $('#font-default').click(function () {
                $('*').each(function () {
                    $(this).css('font-size', $(this).data('original-font-size'));
                });
            });
        });

